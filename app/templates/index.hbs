<div class="ui middle aligned center aligned grid">
    <div style="margin-top: 200px;" class="four wide column">
        <div class="ui fluid card">
            <div class="content">
                <div class="header">flamingo tooling</div>
            </div>
            <div class="content">
                <img src="data:image/png;base64,"
                     class="pixelate ui small image">

                <div class="ui divider"></div>

                <div class="ui two tiny statistics">
                    <div class="statistic">
                        <div class="value">{{numBuilds}}</div>
                        <div class="label">builds</div>
                    </div>
                    <div class="statistic">
                        <div class="value">{{latestVersion}}</div>
                        <div class="label">latest version</div>
                    </div>
                </div>

            </div>
            <div class="extra content">
                {{#link-to 'benchmark' class='ui fluid large teal submit button'}}view benchmarks{{/link-to}}
            </div>
        </div>
    </div>
</div>

{{outlet}}
